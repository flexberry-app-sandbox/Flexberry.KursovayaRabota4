<h3 class="ui header">{{t "forms.i-i-s-kursovaya-rabota-4-просмотр-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya-rabota-4-просмотр-e.спрСтуд-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.спрСтуд
      displayAttributeName="фио студента"
      autocomplete=true
      relationName="спрСтуд"
      projection="СпрСтудL"
      title=(t "forms.i-i-s-kursovaya-rabota-4-просмотр-e.спрСтуд-caption")
      readonly=readonly
      componentName="спрСтудLookup"
      class=(if (v-get validationModel "спрСтуд" "isInvalid") "error")
      data-test-i-i-s-kursovaya-rabota-4-просмотр-e-спрСтуд=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "спрСтуд" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.спрСтуд.фио
      class=(if (v-get validationModel.спрСтуд "фио" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya-rabota-4-просмотр-e.спрСтуд.фио-caption")
      data-test-i-i-s-kursovaya-rabota-4-просмотр-e-спрСтуд-фио=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.спрСтуд "фио" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.спрСтуд.спрГруппа.назв
      class=(if (v-get validationModel.спрСтуд.спрГруппа "назв" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya-rabota-4-просмотр-e.спрСтуд.спрГруппа.назв-caption")
      data-test-i-i-s-kursovaya-rabota-4-просмотр-e-спрСтуд-спрГруппа-назв=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.спрСтуд.спрГруппа "назв" "message")}}
  </div>
</form>
